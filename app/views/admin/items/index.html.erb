<h3>商品一覧</h3>

<%= @items.each do |item| %>

 <p>商品ID</p>
 <span><%= item.id %></span>

 <p>商品名</p>
 <span><%= item.name %></span>

 <p>税抜価格</p>
 <span><%= item.price %></span>

 <p>ジャンル</p>
 <span><%= item.genre_id %></span>

 <p>ステータス</p>
 <span><%= item.is_active %></span>

 <% end %>

 <%= link_to "商品登録", new_admin_item_path %>